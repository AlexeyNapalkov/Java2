<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="lesson4.ChatWindow">
  <grid id="27dc6" binding="rootPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="502" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="2b267" binding="usrPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="line"/>
        <children>
          <component id="a345e" class="javax.swing.JTextArea" binding="usrMsg">
            <constraints border-constraint="Center"/>
            <properties>
              <lineWrap value="true"/>
              <rows value="1"/>
              <wrapStyleWord value="true"/>
            </properties>
          </component>
          <component id="2fc84" class="javax.swing.JButton" binding="btnSend">
            <constraints border-constraint="East"/>
            <properties>
              <text value="Send"/>
            </properties>
          </component>
        </children>
      </grid>
      <component id="7742c" class="javax.swing.JTextArea" binding="flowChat">
        <constraints border-constraint="Center"/>
        <properties>
          <lineWrap value="true"/>
          <wrapStyleWord value="true"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
